import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        int e=a+c;
        float f=(int)e/2.0f;
        int g=b+d;
        float h=(int)g/2.0f;
        System.out.println("Arun's house is located at("+f+","+h+")");
        
    }
}
